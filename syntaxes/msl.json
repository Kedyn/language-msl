          "name": "meta.alias.code.mrc",
          "begin": "(?i)^(alias)\\x20+(?:(-l)\\x20+)?(?!-l)([^\\s]+)\\s*",
          "beginCaptures": {
            "1": {
              "name": "storage.type.alias.mrc"
